    <None Include="wwwroot\app\components\app.component.ts" />
    <None Include="wwwroot\app\components\login\login.component.js" />
    <None Include="wwwroot\app\components\login\login.component.js.map" />
    <None Include="wwwroot\app\components\login\login.component.ts" />
    <None Include="wwwroot\app\main.js" />
