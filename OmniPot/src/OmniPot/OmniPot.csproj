    <None Include="wwwroot\app\viewmodels\admin\users\userdetails.js" />
    <None Include="wwwroot\app\viewmodels\admin\users\userdetails.js.map" />
    <None Include="wwwroot\app\viewmodels\admin\users\userdetails.ts" />
  </ItemGroup>

</Project>
